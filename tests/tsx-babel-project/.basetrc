{
    "specs": "**/*.spec.ts?(x)",
    "bases": "**/*.spec.md",
    "plugins": {
        ".spec.tsx?$": {
            "environment": "baset-env-browser",
            "readers": ["baset-reader-ts", "baset-reader-babel"],
            "resolvers": "baset-resolver-react",
            "baseliner": "baset-baseliner-md"
        }
    }
}
