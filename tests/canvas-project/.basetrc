{
    "specs": "**/*.spec.ts",
    "bases": "**/*.spec.md",
    "plugins": {
        ".spec.ts$": {
            "environment": "baset-env-browser",
            "readers": "baset-reader-ts",
            "resolvers": "baset-resolver-pixi",
            "baseliner": "baset-baseliner-md"
        }
    },
    "options": {
        "baset-env-browser": {
            "serverPort": 3000
        }
    }
}
