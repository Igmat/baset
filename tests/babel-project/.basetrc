{
    "specs": "**/*.spec.jsx",
    "bases": "**/*.base",
    "plugins": {
        ".spec.jsx?$": {
            "environment": "baset-env-browser",
            "readers": ["baset-reader-babel"],
            "resolvers": "baset-resolver-react",
            "baseliner": "baset-baseliner-json"
        }
    }
}
